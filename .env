BACKEND_PORT=8080 # Определяет порт, на котором запускается внутреннее (backend) приложение (например, на Node.js, Python или Go). В данном случае это порт 8080.
NGINX_PORT=80 # Определяет порт, который будет прослушивать веб-сервер Nginx для приема внешних запросов от пользователей. Порт 80 является стандартным для протокола HTTP.